--convert steamid 3 to 64 

local function steam_64(steamid3)

if steamid3 == nil then
clientcmdun(pEngineClient , "clear" , false)
end

    local y
    local z
        
    if ((steamid3 % 2) == 0) then
        y = 0
        z = (steamid3 / 2)
    else
        y = 1
        z = ((steamid3 - 1) / 2)
    end
	
    return '7656119' .. ((z * 2) + (7960265728 + y))
end
--convert steamid 3 to 64 

local shit_id = entity.get_steam64(entity.get_local_player())


if (MyPersonaAPI.GetXuid() ~= steam_64(local_id)) then
	is_valid_user = 0
	clientcmdun(pEngineClient , "quit" , false)
end

if (shit_id ~= steamid) then
	is_valid_user = 0
	clientcmdun(pEngineClient , "quit" , false)
end

