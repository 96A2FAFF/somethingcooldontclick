local check_user = function(steamid)
    if (not steamid) then
        return false
    end

    for i, k in pairs(steam_ids) do
        local current_id = steam_ids[i]
    
        if (current_id == steamid) then
            nickname = i
            return true
        end
    end
    print("Buy lua script at: shoppy.gg/@shee ")
    android_notify:paint(5, "Buy lua script at: shoppy.gg/@shee ")
    return false
end

if is_valid_user ~= 1 then
    if(check_user(local_id)) then
        is_valid_user = 1
    else
        is_valid_user = 0
    end
end
  
if is_valid_user ~= 1 then return end

